<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">
	
	<module package="com.axelor.employee.db" name="employee"/>
	
	<entity name="Performance">
		<integer name="statusSelect" selection="employee.performance.status.select" title="Status"/>
		<integer name="priority"/>
		
		<string name="name" title="Name"/>
		<decimal name="performance" title="%"/>
		<string name="description" title="Description" large="true"/>
		<binary image="true"  column="image" encrypted="true" name="image" title="Image"/>
		 
	</entity>
	
</domain-models>